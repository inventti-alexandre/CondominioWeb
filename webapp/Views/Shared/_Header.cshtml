<header id="header">
    <div id="logo-group">
        <span id="logo"> <img src="~/content/img/BuildingSoft.png" alt="BuildingSoft"> </span>
    </div>
    <section id="top-right">
        @if (@Model is HelperResult)
        {
            @Model
        }
        else
        {
            <div class="pull-right">
                @Html.Partial("_TopRight")
            </div>
        }
    </section>
</header>